{
  "name": "minube/geo-timezone",
  "homepage": "http://minube.com",
  "description": "PHP library for timezone calculation from a particular location and date.",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/phayes/geoPHP",
      "autoload": {
        "files": [
          "vendor/phayes/geophp/geoPHP.inc"
        ]
      }
    },
    {
      "type": "package",
      "package": {
        "name": "guzzle/guzzle",
        "version":"6.3.0",
        "source": {
          "url": "https://github.com/guzzle/guzzle",
          "type": "git",
          "reference":"6.3.0"
        }
      }
    }
  ],
  "require": {
    "phayes/geoPHP": "1.2.*",
    "php": ">=7.0",
    "ext-dom": ">=7.0",
    "ext-mbstring": ">=7.0",
    "ext-zip": "*",
    "ext-xml":">=7.0",
    "phpunit/phpunit": "5.7.4",
    "guzzle/guzzle": "6.3.0"
  },
  "autoload": {
    "psr-4": {
      "GeoTimeZone\\": "src/GeoTimeZone/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "vendor-dir": "vendor"
  }
}
